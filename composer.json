{
  "name": "webeleven/laravel-easymutators",
  "description": "Value object mutation solution for Laravel 5.",
  "keywords": ["photo", "image", "upload", "laravel", "eloquent", "value object", "mutators"],
  "homepage": "https://github.com/webeleven/laravel-easymutators",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/webeleven/eloquent-value-object"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "laravel/framework": "^5.1|^9.0|^10.0",
    "intervention/image": "^2.3",
    "webeleven/eloquent-value-object": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit" : "^9.0"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Webeleven\\EasyMutators\\": "src/"
    },
    "classmap": [
      "tests"
    ]
  },
  "extra": {
      "laravel": {
          "providers": [
              "Webeleven\\EasyMutators\\EasyMutatorsServiceProvider"
          ]
      }
  },
  "authors": [
    {
      "name": "Felipe",
      "email": "felipe.ralc@gmail.com"
    },
    {
      "name": "Thiago",
      "email": "tdrcavallini@gmail.com"
    }
  ],
  "config": {
    "allow-plugins": {
      "kylekatarnls/update-helper": true
    }
  }
}
